    <name>fact</name>
    <modules>
        <module>fact-reports</module>
        <module>fact-portal</module>
        <module>fact-logic</module>
    </modules>
  
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <apache.wicket.version>6.4.0</apache.wicket.version>
        <spring.version>3.2.0.RELEASE</spring.version>
        <spring-data-jpa.version>1.2.0.RELEASE</spring-data-jpa.version>
        <jpa.version>2.0.0</jpa.version>
        <junit.version>4.11</junit.version>
        <hibernate.version>4.1.4.Final</hibernate.version>
        <slf4j.version>1.6.4</slf4j.version>
        <aspectj.version>1.6.12</aspectj.version>
        <jodatime.version>2.1</jodatime.version>
        <mysql.version>5.1.14</mysql.version>
        <commons-dbcp.version>1.2.2</commons-dbcp.version>
        <wicket.version>6.4.0</wicket.version>
        <hamcrest.version>1.1</hamcrest.version>
        <hibernate-jpa.version>1.0.0.Final</hibernate-jpa.version>
        <jasper-reports.version>5.0.0</jasper-reports.version>
        <jfree-chart.version>1.0.12</jfree-chart.version>
        <wicket-jquery-ui.version>6.8.1</wicket-jquery-ui.version> <!-- or 1.5.10 -->
        <apache-commons-collections.version>3.2.0</apache-commons-collections.version>
