            <version>1.2.16</version>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>net.sf.jasperreports</groupId>
            <artifactId>jasperreports</artifactId>
            <version>5.0.1</version>
            <type>jar</type>
        </dependency>
    </dependencies>
   

    <description>Employment Council project is a project for all the companies under the Employment Council</description>
